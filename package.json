    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.7.1",
        "axios": "^1.7.9",
        "electron-icon-builder": "^2.0.1",
        "electron-prompt": "^1.7.0",
        "pinia": "^2.3.0",
        "uuid": "^11.0.3",
